<group name="PEARL-RAHKSHI_GROUP">
  <shader>
    <group group_name="DirtOnScratchMixGroup" name="PearlGroupDirtOnScratchMixGroup">
      <input name="Fac" type="float"/>
      <input name="Shader" type="closure"/>
      <input name="DirtShader" type="closure"/>
      <input name="Vector" type="vector"/>
      <output name="Shader" type="closure"/>
    </group>
    <group group_name="UVGroup" name="PearlGroupUVGroup">
      <input name="InColor" type="color"/>
      <input name="WorldPos" type="vector"/>
      <input name="MinRoughness" type="float"/>
      <input name="MaxRoughness" type="float"/>
      <output name="OutColor" type="color"/>
      <output name="OutRoughness" type="float"/>
    </group>
    <group group_name="ScratchGroup" name="PearlGroupScratchGroup">
      <input name="WorldPos" type="vector"/>
      <input name="Normal" type="vector"/>
      <input name="Distance" type="float" value="0.100000"/>
      <output name="Normal" type="vector"/>
      <output name="DirtStrength" type="float"/>
      <output name="DirtBSDF" type="closure"/>
    </group>
    <group group_name="NormalGroup" name="PearlGroupNormalGroup">
      <output name="Normal" type="vector"/>
    </group>
    <group group_name="STICKER-GROUP" name="Group">
      <input name="Shader" type="closure"/>
      <output name="Shader" type="closure"/>
    </group>
    <mix_closure name="PearlGroupMixShader">
      <input name="Fac" type="float" value="0.100000"/>
    </mix_closure>
    <glossy_bsdf distribution="ggx" name="PearlGroupGlossyBSDF">
      <input name="Color" type="color" value="0.04 0.075 0.15"/>
      <input name="Roughness" type="float" value="0.320000"/>
    </glossy_bsdf>
    <vector_transform convert_from="object" convert_to="world" name="PearlGroupVectorTransform" type="point"/>
    <texture_coordinate name="PearlGroupTextureCoordinate"/>
    <principled_bsdf distribution="ggx" name="Rahkshi" subsurface_method="burley">
      <input name="Specular" type="float" value="0.700000"/>
      <input name="SpecularTint" type="float" value="0.000000"/>
      <input name="Anisotropic" type="float" value="0.000000"/>
      <input name="AnisotropicRotation" type="float" value="0.000000"/>
      <input name="Sheen" type="float" value="0.000000"/>
      <input name="SheenTint" type="float" value="0.500000"/>
      <input name="ClearcoatRoughness" type="float" value="0.000000"/>
      <input name="IOR" type="float" value="1.490000"/>
      <input name="Transmission" type="float" value="0.000000"/>
      <input name="TransmissionRoughness" type="float" value="0.000000"/>
      <input name="ClearcoatNormal" type="vector" value="0.0 0.0 0.0"/>
      <input name="Tangent" type="vector" value="0.0 0.0 0.0"/>
    </principled_bsdf>
    <group_input name="GroupInput"/>
    <group_output name="GroupOutput"/>
    <connect from_node="PearlGroupTextureCoordinate" from_socket="Object" to_node="PearlGroupVectorTransform" to_socket="Vector"/>
    <connect from_node="GroupInput" from_socket="SubsurfaceColor" to_node="PearlGroupGlossyBSDF" to_socket="Color"/>
    <connect from_node="GroupInput" from_socket="Color" to_node="PearlGroupUVGroup" to_socket="InColor"/>
    <connect from_node="PearlGroupVectorTransform" from_socket="Vector" to_node="PearlGroupUVGroup" to_socket="WorldPos"/>
    <connect from_node="PearlGroupScratchGroup" from_socket="Normal" to_node="Rahkshi" to_socket="Normal"/>
    <connect from_node="PearlGroupVectorTransform" from_socket="Vector" to_node="PearlGroupScratchGroup" to_socket="WorldPos"/>
    <connect from_node="GroupInput" from_socket="MaxRoughness" to_node="PearlGroupUVGroup" to_socket="MaxRoughness"/>
    <connect from_node="GroupInput" from_socket="MinRoughness" to_node="PearlGroupUVGroup" to_socket="MinRoughness"/>
    <connect from_node="Rahkshi" from_socket="BSDF" to_node="PearlGroupMixShader" to_socket="Shader1"/>
    <connect from_node="Rahkshi" from_socket="BSDF" to_node="PearlGroupMixShader" to_socket="Shader2"/>
    <connect from_node="PearlGroupUVGroup" from_socket="OutColor" to_node="Rahkshi" to_socket="BaseColor"/>
    <connect from_node="PearlGroupUVGroup" from_socket="OutColor" to_node="Rahkshi" to_socket="SubsurfaceColor"/>
    <connect from_node="PearlGroupScratchGroup" from_socket="Normal" to_node="Rahkshi" to_socket="Normal"/>
    <connect from_node="PearlGroupUVGroup" from_socket="OutRoughness" to_node="Rahkshi" to_socket="Roughness"/>
    <connect from_node="PearlGroupMixShader" from_socket="Shader" to_node="PearlGroupDirtOnScratchMixGroup" to_socket="Shader"/>
    <connect from_node="GroupInput" from_socket="Subsurface" to_node="Rahkshi" to_socket="Subsurface"/>
    <connect from_node="GroupInput" from_socket="Clearcoat" to_node="Rahkshi" to_socket="Clearcoat"/>
    <connect from_node="PearlGroupDirtOnScratchMixGroup" from_socket="Shader" to_node="Group" to_socket="Shader"/>
    <connect from_node="PearlGroupScratchGroup" from_socket="DirtStrength" to_node="PearlGroupDirtOnScratchMixGroup" to_socket="Fac"/>
    <connect from_node="PearlGroupScratchGroup" from_socket="DirtBSDF" to_node="PearlGroupDirtOnScratchMixGroup" to_socket="DirtShader"/>
    <connect from_node="GroupInput" from_socket="Metallic" to_node="Rahkshi" to_socket="Metallic"/>
    <connect from_node="PearlGroupVectorTransform" from_socket="Vector" to_node="PearlGroupDirtOnScratchMixGroup" to_socket="Vector"/>
    <connect from_node="PearlGroupNormalGroup" from_socket="Normal" to_node="PearlGroupScratchGroup" to_socket="Normal"/>
    <connect from_node="GroupInput" from_socket="SubsurfaceRadius" to_node="Rahkshi" to_socket="SubsurfaceRadius"/>
    <connect from_node="GroupInput" from_socket="SubsurfaceRoughness" to_node="Rahkshi" to_socket="Roughness"/>
    <connect from_node="Group" from_socket="Shader" to_node="GroupOutput" to_socket="BSDF"/>
    <connect from_node="GroupInput" from_socket="Transmission" to_node="Rahkshi" to_socket="Transmission"/>
  </shader>
</group>
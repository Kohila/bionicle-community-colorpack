<group name="HOLLOW-GROUP">
  <shader>
    <transparent_bsdf name="Trans">
      <input name="Color" type="color"/>
      <input name="Roughness" type="float" value="0.050000"/>
      <input name="IOR" type="float" value="1.450"/>
    </transparent_bsdf>
    <emission name="EmissionColor">
      <input name="Color" type="color"/>
      <input name="Strength" type="float" value="5.000000"/>
    </emission>
    <layer_weight name="LayerWeight">
      <input name="Blend" type="float" value="0.50000"/>
    </layer_weight>
    <mix_closure name="MixShader">
      <input name="Fac" type="float" value="0.050000"/>
    </mix_closure>
    <group_input name="Input"/>
    <group_output name="GroupOutput"/>
    <connect from_node="Input" from_socket="Color" to_node="Trans" to_socket="Color"/>
    <connect from_node="Input" from_socket="Color" to_node="EmissionColor" to_socket="Color"/>
    <connect from_node="LayerWeight" from_socket="Facing" to_node="MixShader" to_socket="Fac"/>
    <connect from_node="Trans" from_socket="BSDF" to_node="MixShader" to_socket="Shader1"/>
    <connect from_node="EmissionColor" from_socket="Emission" to_node="MixShader" to_socket="Shader2"/>
    <connect from_node="MixShader" from_socket="Shader" to_node="GroupOutput" to_socket="Shader"/>
  </shader>
</group>
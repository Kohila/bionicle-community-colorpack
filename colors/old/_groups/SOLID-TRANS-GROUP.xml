<group name="SOLID-TRANS-GROUP">
  <shader>
    <principled_bsdf distribution="ggx" name="SolidGroupPrincipledBSDF" subsurface_method="burley">
      <input name="Subsurface" type="float" value="0.000000"/>
      <input name="SubsurfaceRadius" type="vector" value="1.0 1.0 1.0"/>
      <input name="Metallic" type="float" value="0.000000"/>
      <input name="Specular" type="float" value="0.300000"/>
      <input name="SpecularTint" type="float" value="0.000000"/>
      <input name="Anisotropic" type="float" value="0.000000"/>
      <input name="AnisotropicRotation" type="float" value="0.000000"/>
      <input name="Sheen" type="float" value="0.000000"/>
      <input name="SheenTint" type="float" value="0.500000"/>
      <input name="Clearcoat" type="float" value="1.000000"/>
      <input name="ClearcoatRoughness" type="float" value="0.000000"/>
      <input name="IOR" type="float" value="1.450000"/>
      <input name="Transmission" type="float" value="0.000000"/>
      <input name="TransmissionRoughness" type="float" value="0.000000"/>
      <input name="Tangent" type="vector" value="0.0 0.0 0.0"/>
    </principled_bsdf>
    <group group_name="NormalGroup" name="SolidGroupNormalGroup">
      <output name="Normal" type="vector"/>
    </group>
    <group group_name="DirtOnScratchMixGroup" name="SolidGroupDirtOnScratchMixGroup">
      <input name="Fac" type="float"/>
      <input name="Shader" type="closure"/>
      <input name="DirtShader" type="closure"/>
      <input name="Vector" type="vector"/>
      <output name="Shader" type="closure"/>
    </group>
    <group group_name="ScratchGroup" name="SolidGroupScratchGroup">
      <input name="WorldPos" type="vector"/>
      <input name="Normal" type="vector"/>
      <input name="Distance" type="float" value="0.050000"/>
      <output name="Normal" type="vector"/>
      <output name="DirtStrength" type="float"/>
      <output name="DirtBSDF" type="closure"/>
    </group>
    <group group_name="UVGroup" name="SolidGroupUVGroup">
      <input name="InColor" type="color"/>
      <input name="WorldPos" type="vector"/>
      <input name="MinRoughness" type="float"/>
      <input name="MaxRoughness" type="float"/>
      <output name="OutColor" type="color"/>
      <output name="OutRoughness" type="float"/>
    </group>
    <group group_name="STICKER-GROUP" name="Group">
      <input name="Shader" type="closure"/>
      <output name="Shader" type="closure"/>
    </group>
    <value name="MinRoughness" value="0.05"/>
    <value name="MaxRoughness" value="0.5"/>
    <vector_transform convert_from="object" convert_to="world" name="SolidGroupVectorTransform" type="point"/>
    <texture_coordinate name="SolidGroupTextureCoordinate"/>
    <group_input name="GroupInput"/>
    <group_output name="GroupOutput"/>
    <connect from_node="SolidGroupPrincipledBSDF" from_socket="BSDF" to_node="SolidGroupDirtOnScratchMixGroup" to_socket="Shader"/>
    <connect from_node="SolidGroupTextureCoordinate" from_socket="Object" to_node="SolidGroupVectorTransform" to_socket="Vector"/>
    <connect from_node="SolidGroupUVGroup" from_socket="OutColor" to_node="SolidGroupPrincipledBSDF" to_socket="SubsurfaceColor"/>
    <connect from_node="SolidGroupUVGroup" from_socket="OutColor" to_node="SolidGroupPrincipledBSDF" to_socket="BaseColor"/>
    <connect from_node="SolidGroupUVGroup" from_socket="OutRoughness" to_node="SolidGroupPrincipledBSDF" to_socket="Roughness"/>
    <connect from_node="GroupInput" from_socket="BaseColor" to_node="SolidGroupUVGroup" to_socket="InColor"/>
    <connect from_node="GroupInput" from_socket="Transmission" to_node="SolidGroupPrincipledBSDF" to_socket="Transmission"/>
    <connect from_node="SolidGroupVectorTransform" from_socket="Vector" to_node="SolidGroupUVGroup" to_socket="WorldPos"/>
    <connect from_node="SolidGroupScratchGroup" from_socket="Normal" to_node="SolidGroupPrincipledBSDF" to_socket="Normal"/>
    <connect from_node="SolidGroupScratchGroup" from_socket="Normal" to_node="SolidGroupPrincipledBSDF" to_socket="ClearcoatNormal"/>
    <connect from_node="SolidGroupVectorTransform" from_socket="Vector" to_node="SolidGroupScratchGroup" to_socket="WorldPos"/>
    <connect from_node="MinRoughness" from_socket="Value" to_node="SolidGroupUVGroup" to_socket="MinRoughness"/>
    <connect from_node="MaxRoughness" from_socket="Value" to_node="SolidGroupUVGroup" to_socket="MaxRoughness"/>
    <connect from_node="SolidGroupDirtOnScratchMixGroup" from_socket="Shader" to_node="Group" to_socket="Shader"/>
    <connect from_node="SolidGroupScratchGroup" from_socket="DirtStrength" to_node="SolidGroupDirtOnScratchMixGroup" to_socket="Fac"/>
    <connect from_node="SolidGroupScratchGroup" from_socket="DirtBSDF" to_node="SolidGroupDirtOnScratchMixGroup" to_socket="DirtShader"/>
    <connect from_node="SolidGroupVectorTransform" from_socket="Vector" to_node="SolidGroupDirtOnScratchMixGroup" to_socket="Vector"/>
    <connect from_node="SolidGroupNormalGroup" from_socket="Normal" to_node="SolidGroupScratchGroup" to_socket="Normal"/>
    <connect from_node="Group" from_socket="Shader" to_node="GroupOutput" to_socket="Shader"/>
  </shader>
</group>
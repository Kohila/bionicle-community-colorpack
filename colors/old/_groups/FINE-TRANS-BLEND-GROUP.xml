<group name="FINE-TRANS-BLEND-GROUP">
  <shader>
    <glass_bsdf name="Trans">
      <input name="Color" type="color"/>
      <input name="Roughness" type="float" value="0.050000"/>
      <input name="IOR" type="float" value="1.450"/>
    </glass_bsdf>
    <diffuse_bsdf name="Diffuse">
      <input name="Color" type="color"/>
      <input name="Roughness" type="float" value="0.000000"/>
    </diffuse_bsdf>
    <layer_weight name="LayerWeight">
      <input name="Blend" type="float" value="0.200000"/>
    </layer_weight>
    <mix_closure name="MixShader">
      <input name="Fac" type="float" value="0.050000"/>
    </mix_closure>
    <group_input name="Input"/>
    <group_output name="Output"/>
    <group group_name="STICKER-GROUP" name="Group">
      <input name="Shader" type="closure"/>
      <output name="Shader" type="closure"/>
    </group>
    <connect from_node="Input" from_socket="Color" to_node="Trans" to_socket="Color"/>
    <connect from_node="Input" from_socket="Color" to_node="Diffuse" to_socket="Color"/>
    <connect from_node="LayerWeight" from_socket="Fresnel" to_node="MixShader" to_socket="Fac"/>
    <connect from_node="Trans" from_socket="BSDF" to_node="MixShader" to_socket="Shader1"/>
    <connect from_node="Diffuse" from_socket="BSDF" to_node="MixShader" to_socket="Shader2"/>
    <connect from_node="MixShader" from_socket="Shader" to_node="Group" to_socket="Shader"/>
    <connect from_node="Group" from_socket="Shader" to_node="Output" to_socket="Shader"/>
  </shader>
</group>